{
  "name": "ImgurStack",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Proxy service for imgur in stackoverflow domains",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [ "tabs", "*://*.stackoverflow.com/*" ],
  "content_scripts": [
    {
      "matches": [ "*://*.stackoverflow.com/*" ],
      "js": [ "jquery-3.4.1.min.js", "content.js" ]
    }
  ]
}
