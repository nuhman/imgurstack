{
    "name": "Hello",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Says hello to SO",
    "permissions": ["tabs", "*://*.stackoverflow.com/*"],
    "content_scripts": [
      {
        "matches": ["*://*.stackoverflow.com/*"],
        "js": ["jquery-3.4.1.min.js", "content.js"]
      }
    ]
  }